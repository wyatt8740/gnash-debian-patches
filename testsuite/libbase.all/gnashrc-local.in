# Test for override whitelist
set whitelist off

# Don't access content from these sites
# Test for append to list
append blacklist www.gnashdev.org www.wikipedia.de

append localSandboxPath /tmp/gnash

# Use arbitrary renderer
set Renderer fakeRenderer

# Use arbitrary renderer
set MediaHandler fakeMediaHandler

# Override scripts timeout
set scriptsTimeout 2

# Override scripts recursion limit
set scriptsRecursionLimit 32
